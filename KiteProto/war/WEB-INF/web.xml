<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>KiteProtoServlet</servlet-name>
		<servlet-class>me.mandala.kite.KiteProtoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KiteProtoServlet</servlet-name>
		<url-pattern>/mandala</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>KiteProtoArtifactServlet</servlet-name>
		<servlet-class>me.mandala.kite.KiteProtoArtifactServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KiteProtoArtifactServlet</servlet-name>
		<url-pattern>/artifact</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Create-Mandala.html</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>